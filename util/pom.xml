    <modelVersion>4.0.0</modelVersion>
    <properties>
        <junit-platform-launcher.version>1.5.0</junit-platform-launcher.version>
        <junit-jupiter-params.version>5.5.0</junit-jupiter-params.version>
    </properties>
    <parent>
        <groupId>automenta</groupId>
        <artifactId>narchy</artifactId>
        <version>1.0</version>
