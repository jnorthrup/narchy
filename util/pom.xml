        <dependency>
            <groupId>org.gridkit.lab</groupId>
            <artifactId>telecontrol-ssh</artifactId>
            <version>0.8.16</version>
            <scope>compile</scope>
        </dependency>
    </dependencies>
    <build>
        <plugins>
            <plugin>
                <groupId>org.pitest</groupId>
                <artifactId>pitest-maven</artifactId>
                <version>1.4.9</version>
                <dependencies>
                    <dependency>
                        <groupId>org.pitest</groupId>
                        <artifactId>pitest-junit5-plugin</artifactId>
                        <version>0.9</version>
                    </dependency>
                </dependencies>
                <configuration>
                    <withHistory>false</withHistory>
                    <exportLineCoverage>true</exportLineCoverage>
                    <skipFailingTests>true</skipFailingTests>
                    <outputFormats>html</outputFormats>
                    <testPlugin>junit5</testPlugin>
<!--                    <jvmArgs>-Xmx64m</jvmArgs>-->
                    <targetClasses>
                        <param>jcog.pri.**</param>
                    </targetClasses>
                    <targetTests>
                        <param>jcog.pri.*</param>
                    </targetTests>
                    <verbose>false</verbose>
                    <threads>6</threads>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>
