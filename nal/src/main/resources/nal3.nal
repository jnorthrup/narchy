//// inheritance composition ////////////////////////////////////////////////////////////////////////////////////
//If P and S are in the intension/extension of M, then union/difference and intersection can be built:
//The duality of extension and intension in NAL corresponds to the
//  duality of intersection and union in set theory â€” intensional intersection
//  corresponds to extensional union, and extensional intersection corresponds
//  to intensional union.

    (P --> M), (S --> M), notSet(S), notSet(P), neqRCom(S,P) |- ((P | S) --> M), (Belief:Intersection)
    (P --> M), (S --> M), notSet(S), notSet(P), neqRCom(S,P) |- ((P & S) --> M), (Belief:Union)

    (M --> P), (M --> S), notSet(S), notSet(P), neqRCom(S,P) |- (M --> (P & S)), (Belief:Intersection)
    (M --> P), (M --> S), notSet(S), notSet(P), neqRCom(S,P) |- (M --> (P | S)), (Belief:Union)

    (P --> M), (S --> M), notSet(S), notSet(P), neqRCom(S,P) |- ((P ~ S) --> M), (Belief:Difference)
    (P --> M), (S --> M), notSet(S), notSet(P), neqRCom(S,P) |- ((S ~ P) --> M), (Belief:DifferenceReverse)

    (M --> P), (M --> S), notSet(S), notSet(P), neqRCom(S,P) |- (M --> (P - S)), (Belief:Difference)
    (M --> P), (M --> S), notSet(S), notSet(P), neqRCom(S,P) |- (M --> (S - P)), (Belief:DifferenceReverse)

        //(P --> M), (S --> M), is(P,"+"), is(S,"+") |- (differ(P,S) --> M), (Belief:Difference)
        //((A,B) --> M), ((X,Y) --> M), is(A,"+"), is(B,"+"), is(X,"+"), is(Y,"+") |- ((intersect(A,X),intersect(B,Y)) --> M), (Belief:Intersection)
        //((A,B) --> M), ((X,Y) --> M), is(A,"+"), is(B,"+"), is(X,"+"), is(Y,"+") |- ((union(A,X),union(B,Y)) --> M), (Belief:Union)


//// inheritance decomposition ////////////////////////////////////////////////////////////////////////////////////
//if (S --> M) is the case, and ((|,S,A..+) --> M) is not the case, then ((|,A..+) --> M) is not the case, hence Belief:DecomposePositiveNegativeNegative
//TODO maybe X-S should work like set difference, not 'without(' current semantics
     (X --> M), (S --> M), is(S,"|"), subOf(S,X) |- (without(S,X) --> M), (Belief:DecomposePositiveNegativeNegative)
         //(X-->M), (S --> N), is(S,"|"), subOf(S,X), neq(M,N) |- subIfUnifiesAny((without(S,X) --> N), N, M), (Belief:DecomposePositiveNegativeNegative)
     (X --> M), (S --> M), is(S,"&"), subOf(S,X) |- (without(S,X) --> M), (Belief:DecomposeNegativePositivePositive)
         //(X-->M), (S --> N), is(S,"&"), subOf(S,X), neq(M,N) |- subIfUnifiesAny((without(S,X) --> N), N, M), (Belief:DecomposeNegativePositivePositive)

     (M --> X), (M --> S), is(S,"&"), subOf(S,X) |- (M --> without(S,X)), (Belief:DecomposePositiveNegativeNegative)
        //(M --> X), (N --> S), is(S,"&"), subOf(S,X), neq(M,N) |- subIfUnifiesAny((N --> without(S,X)), N, M), (Belief:DecomposePositiveNegativeNegative)
     (M --> X), (M --> S), is(S,"|"), subOf(S,X) |- (M --> without(S,X)), (Belief:DecomposeNegativePositivePositive)
        //(M --> X), (N --> S), is(S,"|"), subOf(S,X), neq(M,N) |- subIfUnifiesAny((N --> without(S,X)), N, M), (Belief:DecomposeNegativePositivePositive)

     //DIFFi
     (M --> S), (M --> (S - P)) |- (M --> P), (Belief:DecomposePositiveNegativePositive)
     (M --> P), (M --> (S - P)) |- (M --> S), (Belief:DecomposeNegativeNegativeNegative)
        (M --> S), (M --> (S - P)) |- (M --> P), (Belief:DecomposePositivePositiveNegative)
        (M --> P), (M --> (S - P)) |- (M --> S), (Belief:DecomposeNegativePositivePositive)

     //DIFFe
     (S --> M), ((S ~ P) --> M) |- (P --> M), (Belief:DecomposePositiveNegativePositive)
     (P --> M), ((S ~ P) --> M) |- (S --> M), (Belief:DecomposeNegativeNegativeNegative)
        (S --> M), ((S ~ P) --> M) |- (P --> M), (Belief:DecomposePositivePositiveNegative)
        (P --> M), ((S ~ P) --> M) |- (S --> M), (Belief:DecomposeNegativePositivePositive)

    //Raw DIFFe
    S, (S ~ P) |- P, (Belief:DecomposePositiveNegativePositive)
        S, (S ~ P) |- P, (Belief:DecomposePositivePositiveNegative)
        P, (S ~ P) |- S, (Belief:DecomposeNegativePositivePositive)
    P, (S ~ P) |- S, (Belief:DecomposeNegativeNegativeNegative)

     //similarity (S==P)! (symmetric: DecomposeNegative....)
     ((S ~ P) --> M), (S --> M) |- (P --> M), (Goal:DecomposeNegativePositivePositive, Time:TaskImmediate)
     (M --> (S - P)), (M --> S) |- (M --> P), (Goal:DecomposeNegativePositivePositive, Time:TaskImmediate)
     (S ~ P),         S         |- P,         (Goal:DecomposeNegativePositivePositive, Time:TaskImmediate)
        ((S ~ P) --> M), (P --> M) |- (S --> M), (Goal:DecomposeNegativePositivePositive, Time:TaskImmediate)
        (M --> (S - P)), (M --> P) |- (M --> S), (Goal:DecomposeNegativePositivePositive, Time:TaskImmediate)
        (S ~ P),         P         |- S,         (Goal:DecomposeNegativePositivePositive, Time:TaskImmediate)
           ((S ~ P) --> M), (S --> M) |- (P --> M), (Goal:DecomposeNegativeNegativeNegative, Time:TaskImmediate)
           (M --> (S - P)), (M --> S) |- (M --> P), (Goal:DecomposeNegativeNegativeNegative, Time:TaskImmediate)
           (S ~ P),         S         |- P,         (Goal:DecomposeNegativeNegativeNegative, Time:TaskImmediate)
              ((S ~ P) --> M), (P --> M) |- (S --> M), (Goal:DecomposeNegativeNegativeNegative, Time:TaskImmediate)
              (M --> (S - P)), (M --> P) |- (M --> S), (Goal:DecomposeNegativeNegativeNegative, Time:TaskImmediate)
              (S ~ P),         P         |- S,         (Goal:DecomposeNegativeNegativeNegative, Time:TaskImmediate)

     //difference --(S==P)! (asymmetric: DecomposePositive... )
     ((S ~ P) --> M), (S --> M) |- (P --> M), (Goal:DecomposePositivePositiveNegative, Time:TaskImmediate)
     (M --> (S - P)), (M --> S) |- (M --> P), (Goal:DecomposePositivePositiveNegative, Time:TaskImmediate)
     (S ~ P),         S         |- P,         (Goal:DecomposePositivePositiveNegative, Time:TaskImmediate)
        ((S ~ P) --> M), (P --> M) |- (S --> M), (Goal:DecomposePositiveNegativePositive, Time:TaskImmediate)
        (M --> (S - P)), (M --> P) |- (M --> S), (Goal:DecomposePositiveNegativePositive, Time:TaskImmediate)
        (S ~ P),         P         |- S,         (Goal:DecomposePositiveNegativePositive, Time:TaskImmediate)



     //intersection conditional goal decompose
     ((|,P,S,A..*) --> M), (S --> M) |- (P --> M), (Goal:DecomposePositivePositivePositive, Time:TaskImmediate)
     ((&,P,S,A..*) --> M), (S --> M) |- (P --> M), (Goal:DecomposePositiveNegativePositive, Time:TaskImmediate)
     (M --> (&,P,S,A..*)), (M --> S) |- (M --> P), (Goal:DecomposePositivePositivePositive, Time:TaskImmediate)
     (M --> (|,P,S,A..*)), (M --> S) |- (M --> P), (Goal:DecomposePositiveNegativePositive, Time:TaskImmediate)


